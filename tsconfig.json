{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "outDir": "./build",
    "baseUrl": "./",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "resolveJsonModule":true
  },
  "include": [
    ".env",
    "src/**/*.ts"
, "seed.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
